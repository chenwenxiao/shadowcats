extend layout

block content
  div.raw-fluid.clearfix
    div.span6.ui-sortable
      svg#svg
    div.span5.ui-sortable
      pre#editor.
        six = (one = 10) + (two = 20) + (three = 30)
        solve.left 65
        solve.use solve.map.items[1]
        solve.up 30
        list = [1, 2, 3, 4, 5]
        race = (winner, runners...) ->
          print winner, runners
      p
        button#run.btn.btn-primary.btn-large 运行
        button#test.btn test
        button#go.btn go

  script(data-main="javascripts/main.js", src="/libs/requirejs/require.js")

  style(type='text/css', media='screen').
    body {
      width: 100%;
      height: 100%;
      background-color: #aaa;
    }
    #editor {
      margin: 0;
      position: relative;
      top: 0;
      bottom: 0;
      left: 0;
      right: 0;
      width: 100%;
      height: 500px;
    }
    #svg {
      margin: 0;
      position: relative;
      top: 0;
      bottom: 0;
      left: 0;
      right: 0;
      width: 600px;
      height: 600px;
    }
    #btn {

    }
    .ace_gutter-cell.ace_breakpoint{
      background-image: url(images/breakpoint.png);
    }
